{
    "name": "HS.fi Dark mode",
    "version": "1.5.5",
    "description": "Dark theme for HS.fi",
    "author": "O. Inha",
    "permissions": ["tabs", "activeTab", "*://*.hs.fi/*"],
    "content_scripts": [
        {
          "matches": ["*://*.hs.fi/*"],
          "css": ["hs-dark.css"],
          "js": ["content_script.js"],
          "run_at" : "document_start"
        }
    ],
    "browser_action": {},
    "manifest_version": 2
}
