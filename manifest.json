{
    "name": "HS.fi Dark mode",
    "version": "1.5.8",
    "description": "Dark theme for HS.fi",
    "author": "O. Inha",
    "permissions": ["tabs", "activeTab"],
    "content_scripts": [
        {
          "matches": ["*://*.hs.fi/*"],
          "css": ["hs-dark.css"],
          "js": ["content_script.js"],
          "run_at" : "document_start",
          "all_frames": true
        }
    ],
    "action": {},
    "host_permissions": [
      "*://*.hs.fi/*"
    ],
    "manifest_version": 3
}
